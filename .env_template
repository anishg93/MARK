# Copy this file as .env in the same directory and fill all the values
AZURE_OPENAI_BASE_URL=<Azure openai base url>
AZURE_OPENAI_API_KEY=<Azure openai  key>
AZURE_OPENAI_DEPLOYMENT_NAME=<Azure openai deployment name>
AZURE_OPENAI_MODEL_NAME=<Azure openai model name>
AZURE_OPENAI_API_VERSION=<Azure openai api version>
CHAINLIT_USERNAME=<chainlit username>
CHAINLIT_PASSWORD=<chainlit password>
CHAINLIT_ROLE=<chainlit role>
CHAINLIT_AUTH_SECRET=<chainlit auth secret, generated from chainlit>
AIPROJECT_CONNECTION_STRING=<Azure AI Foundry connection string, only needed for LLM based evaluation>
EVALUATION_MODEL=<evaluation model name, only needed for LLM based evaluation>